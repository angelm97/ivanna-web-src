<title>{page_title}</title>
<META NAME="Keywords" CONTENT=" {meta_title}">
<meta NAME="Description" CONTENT=" {meta_description} ">
{HEADER_HTML}
{HEADER_MIDDLE_HTML}
<tr>
    <td valign="top">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr>
                <td class="heading_bar_internal">
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <tr>
                            <td width="15" valign="middle" class="forum_heading_img"> </td>
                            <td valign="middle" class="heading_bar_internal_content">{HEADING_TITLE}</td>
                            <td valign="middle" width="10%" align="right" class="heading_bar_internal_content1">
                                {INFO_TEXT_HEADING_SEARCH}</td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td class="block_with_heading_bar">
                    <!---->
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <tr>
                            <td>{update_message}
                                <table width="100%" border="0" cellpadding="0" cellspacing="1">
                                    <tr>
                                        <td class="forum_sub_heading">
                                            <table width="100%" border="0" cellpadding="0" cellspacing="1">
                                                <tr>
                                                    <td colspan="3">{INFO_TEXT_FORUM_HEADER_LINK}</td>
                                                </tr>
                                                <tr>
                                                    <td colspan="3">&nbsp;</td>
                                                </tr>
                                                <tr>
                                                    <td colspan="3">{new_button}</td>
                                                </tr>
                                                <tr>
                                                    <td valign="middle"></td>
                                                    <td></td>
                                                    <td valign="top">
                                                        <table width="95%" align="center" cellspacing="2"
                                                            cellpadding="2" border="0">
                                                            <tr>{form}{hidden_fields1}
                                                                <td valign="top" class="label" align="right">{total}
                                                                </td>
                                                                <td valign="top" align="right" class="label">
                                                                    {list_page}&nbsp;{page}&nbsp;</td>
                                                            </tr>
                                                        </table>
                                                    </td>
                                                </tr>

                                                <tr>
                                                    <td colspan="3" class="forum_sub_heading1">{forum_title}</td>
                                                </tr>

                                            </table>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <table width="100%" border="0" cellpadding="4" cellspacing="1"
                                                class="forum_box">
                                                <tr class="forum_header_bar">
                                                    <td colspan="2" class="forum_heading" valign="middle">Topic</td>
                                                    <td width="86" class="forum_heading">Replies </td>
                                                    <td class="forum_heading">Author </td>
                                                    <td class="forum_heading">Last Post</span></td>
                                                </tr>
                                                <!-- BEGIN topic -->
                                                <tr {topic.row_selected}>
                                                    <td class="forum_row_content"><img src="../../img/topics2.png"></td>
                                                    <td valign="top" class="forum_row_content">
                                                        <table width="100%" border="0" cellspacing="0" cellpadding="0">
                                                            <tr>
                                                                <td class="forum_title">{topic.topic}</td>
                                                            </tr>
                                                            <tr>
                                                                <td class="forum_content">{topic.description}</td>
                                                            </tr>
                                                        </table>
                                                    </td>
                                                    <td class="forum_row_content1" valign="top">{topic.replies}</td>
                                                    <td class="forum_row_content1" valign="top"><span
                                                            class="forum_author">{topic.author}</span></td>
                                                    <td class="forum_row_content1" valign="top"><span
                                                            class="forum_author">{topic.last_reply_author}</span><br />
                                                        <nobr>{topic.last_post}</nobr>
                                                    </td>
                                                </tr>
                                                <!-- END topic -->
                                            </table>
                                        </td>
                                    </tr>
                                </table>
                                {INFO_TEXT_HIDE_START}
                                <table width="95%" align="center" cellspacing="2" cellpadding="2" border="0">
                                    <tr>
                                        <td valign="top" class="label">{total}</td>
                                        <td valign="top" align="right" class="label">{list_page}&nbsp;{page}&nbsp;</td>
                                    </tr>
                                    </form>
                                </table>
                                {INFO_TEXT_HIDE_END}
                            </td>
                        </tr>
                        <tr>
                            <td></td>
                        </tr>
                        <tr>
                            <td>&nbsp;</td>
                        </tr>
                    </table>
                    <!---->
                </td>
            </tr>
        </table>
    </td>
</tr>
{FOOTER_HTML}