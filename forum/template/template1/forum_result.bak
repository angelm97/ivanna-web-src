                </tr>
               </table>
              </td>
              <td class="forum_row_content1" valign="top">{search_result.reply}</td>
              <td class="forum_row_content1" valign="top"><nobr>{search_result.inserted}</nobr> <span class="forum_author">{search_result.posted_by}</span></td>
              <td class="forum_row_content1" valign="top">{search_result.view}</td>
             </tr>
            <!-- END forum_result -->
            {INFO_TEXT_HIDE_END}
